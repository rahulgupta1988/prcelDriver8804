<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical" >

        <com.parcelsixd.parcel.driver.widget.MyFontTextView
            android:id="@+id/tvBasePriceText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_fp_margin_top"
            android:paddingBottom="@dimen/dimen_fp_margin"
            android:paddingTop="@dimen/dimen_fp_margin"
            android:text="@string/text_invoice"
            android:textColor="@color/color_text_dark"
            android:textSize="@dimen/size_general" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dimen_fp_margin"
            android:paddingTop="@dimen/dimen_fp_margin"
            android:text="aaa"
            android:paddingLeft="@dimen/dimen_fp_margin_top"
            android:background="#fac05f"
            android:id="@+id/payment_type"
            android:textColor="@color/color_text_dark"
            android:textSize="@dimen/size_general" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/line" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/dimen_bill_padding_one"
                    android:paddingLeft="@dimen/dimen_fp_margin_top"
                    android:paddingRight="@dimen/dimen_bill_padding_two"
                    android:paddingTop="@dimen/dimen_bill_padding_one" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_fp_margin_top"
                        android:src="@drawable/dot_fare_estimate" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvBasePriceText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left|center"
                        android:layout_marginRight="@dimen/dimen_bill_margin_two"
                        android:layout_weight="0.6"
                        android:gravity="left"
                        android:text="@string/text_base_price"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_general" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvBasePrice"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:layout_weight="0.4"
                        android:text="600.00"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:background="@drawable/line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:gravity="center"
                    android:paddingBottom="@dimen/dimen_bill_padding_one"
                    android:paddingLeft="@dimen/dimen_fp_margin_top"
                    android:paddingRight="@dimen/dimen_bill_padding_two"
                    android:paddingTop="@dimen/dimen_bill_padding_one" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_fp_margin_top"
                        android:src="@drawable/dot_fare_estimate" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_two"
                        android:layout_weight="0.6"
                        android:orientation="vertical" >

                        <com.parcelsixd.parcel.driver.widget.MyFontTextView
                            android:id="@+id/tvDistCost"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top|right"
                            android:text="@string/text_distance_cost"
                            android:textColor="@color/color_text_dark"
                            android:textSize="@dimen/size_general" />

                        <com.parcelsixd.parcel.driver.widget.MyFontTextView
                            android:id="@+id/tvBillDistancePerMile"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top|right"
                            android:text="@string/text_base_price"
                            android:textColor="@color/color_text_dark"
                            android:textSize="@dimen/size_general" />
                    </LinearLayout>

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvDis1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:layout_weight="0.4"
                        android:gravity="left"
                        android:text="0"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:background="@drawable/line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:gravity="center"
                    android:paddingBottom="@dimen/dimen_bill_padding_one"
                    android:paddingLeft="@dimen/dimen_fp_margin_top"
                    android:paddingRight="@dimen/dimen_bill_padding_two"
                    android:paddingTop="@dimen/dimen_bill_padding_one" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_fp_margin_top"
                        android:src="@drawable/dot_fare_estimate" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_two"
                        android:layout_weight="0.6"
                        android:orientation="vertical" >

                        <com.parcelsixd.parcel.driver.widget.MyFontTextView
                            android:id="@+id/tvTimeCost"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top|right"
                            android:text="@string/text_time_cost"
                            android:textColor="@color/color_text_dark"
                            android:textSize="@dimen/size_general" />

                        <com.parcelsixd.parcel.driver.widget.MyFontTextView
                            android:id="@+id/tvBillTimePerHour"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top|right"
                            android:text="@string/text_base_price"
                            android:textColor="@color/color_text_dark"
                            android:textSize="@dimen/size_general" />
                    </LinearLayout>

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvTime1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:layout_weight="0.4"
                        android:gravity="left"
                        android:text="0"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:background="@drawable/line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:gravity="center"
                    android:paddingBottom="@dimen/dimen_bill_padding_one"
                    android:paddingLeft="@dimen/dimen_fp_margin_top"
                    android:paddingRight="@dimen/dimen_bill_padding_two"
                    android:paddingTop="@dimen/dimen_bill_padding_one" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_fp_margin_top"
                        android:src="@drawable/dot_fare_estimate" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvReferralBonusText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left|center"
                        android:layout_marginRight="@dimen/dimen_bill_margin_two"
                        android:layout_weight="0.6"
                        android:gravity="left"
                        android:text="@string/text_referral_bonus"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_general" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvReferralBonus"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:layout_weight="0.4"
                        android:text="0"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:background="@drawable/line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:gravity="center"
                    android:paddingBottom="@dimen/dimen_bill_padding_one"
                    android:paddingLeft="@dimen/dimen_fp_margin_top"
                    android:paddingRight="@dimen/dimen_bill_padding_two"
                    android:paddingTop="@dimen/dimen_bill_padding_one" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_fp_margin_top"
                        android:src="@drawable/dot_fare_estimate" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvPromoBonusText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left|center"
                        android:layout_marginRight="@dimen/dimen_bill_margin_two"
                        android:layout_weight="0.6"
                        android:gravity="left"
                        android:text="@string/text_promo_bonus"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_general" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvPromoBonus"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:layout_weight="0.4"
                        android:text="0"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_price" />
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_margin_register"
                    android:background="@drawable/line_invoice" />
            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="0.6"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dimen_bill_padding_one"
                android:paddingRight="@dimen/dimen_bill_padding_two" >

                <com.parcelsixd.parcel.driver.widget.MyFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_bill_second_top"
                    android:gravity="center"
                    android:text="@string/text_total"
                    android:textSize="@dimen/size_general"
                    android:textColor="@color/color_text_dark" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_bill_margin_three"
                        android:gravity="center"
                        android:text="@string/payment_unit"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_bill_total" />

                    <com.parcelsixd.parcel.driver.widget.MyFontTextView
                        android:id="@+id/tvTotal1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="60.00"
                        android:textColor="@color/color_text_dark"
                        android:textSize="@dimen/size_bill_total" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:gravity="bottom" >

        <com.parcelsixd.parcel.driver.widget.MyFontButton
            android:id="@+id/btnBillDialogClose"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_done"
            android:textColor="@color/white" />
    </RelativeLayout>

